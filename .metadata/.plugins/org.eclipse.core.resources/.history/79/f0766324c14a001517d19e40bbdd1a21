
public class Testa {

	public static void main(String[] args){

		Teclado tc=new Teclado();

		int n=0;
		String palavra=null;
		
		tc.lerInt();
		tc.lerInt(n);
		tc.lerInt(palavra, 2);
		tc.lerInt(palavra);
		
		tc.lerFloat();
		tc.lerFloat(n);
		tc.lerFloat(palavra, n);
		tc.lerFloat(palavra);
		
		
}
}
